# Alias code-oss to code
if type code-oss >/dev/null ; then
  if ! type code >/dev/null ; then
    alias code='code-oss'
  fi
else
  # Don't redefine on linux
  if ! command -v code >/dev/null ; then
    function code {
      VSCODE_CWD="$PWD" open -n -b "com.microsoft.VSCode" --args "$@"
    }
  fi
fi
